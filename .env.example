# Example environment variables for price-harvester

# Bybit (optional for public market data)
BYBIT_API_KEY=
BYBIT_API_SECRET=

# Telegram bot for alerts (optional)
TELEGRAM_TOKEN=
TELEGRAM_CHAT_ID=

# Database
DATABASE_PATH=./data/price.db

# Logging
LOG_LEVEL=INFO

# Symbols to monitor (comma-separated)
SYMBOLS=BTCUSDT

# Harvest interval in seconds (for REST fallback)
HARVEST_INTERVAL=5

